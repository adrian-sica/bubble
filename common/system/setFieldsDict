FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

defaultFieldValues
(
    volScalarFieldValue alpha.water 1
);

regions
(
    cylinderToCell
    {
        point1 (0.5 0.5 0);
	point2 (0.5 0.5 0.1);
	radius 0.25;
        fieldValues
        (
            volScalarFieldValue alpha.water 0
        );
    }
);


